		},
		{
			"description": "Login Page - browser panel",
			"dclass": ".bottom-panel",
			"width": 451,
			"height":53,
			"background_color": "#e6e6e6"
		},
		{
			"description": "Login Page - ncoreID",
			"dclass": "[data-hook=login-form] [data-hook=ncore-id]",
			"width": 360,
			"height":40,
			"background_color": "#ffffff"
		},
		{
			"description": "Login Page - password",
			"dclass": "[data-hook=login-form] [data-hook=password]",
			"width": 360,
			"height":40,
			"background_color": "#ffffff"
		}		
	],
	"error": [
		{
			"description": "Login Page - ErrorBox",
			"dclass": ".main-panel .form-error-box",
			"width": 360,
			"height":30,
			"background_color": "#ffebeb"
		},
		{
			"description": "Login Page - Form Error",
			"dclass": "[data-hook=form-error]",
			"font_family": "Roboto",
			"font_size":"12px",
			"font_weight": "400",
			"font_style": "normal",
			"font_stretch": "100%",
			"color": "#f43736"
		},
		{
			"description": "Login Page - warning icon",
			"dclass": ".main-panel .form-error-box .warning",
			"width": 14,
			"height":14
		},
		{
			"description": "Login Page - ncoreID error",
			"dclass": "[data-hook=login-form] [data-hook=ncore-id].error-input",
			"width": 360,
			"height":40,
			"font_family": "Roboto",
			"font_size":"14px",
			"font_weight": "400",
			"color": "#f43736"
		},
		{
			"description": "Login Page - password error",
			"dclass": "[data-hook=login-form] [data-hook=password].error-input",
			"width": 360,
			"height":40,

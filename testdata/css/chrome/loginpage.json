{
	"ctrl": [
		{
			"ncoreid": "input[name='ncore-id']",
			"password": "input[name='pass']",
			"login_button": "input[name='login']",
			"forgot_pass": ".login-help a",
			"loggedin": "[data-id=ContainerMe] [title=Available]",
			"nameOkta": "[id=okta-signin-username]",
			"passOkta": "[id=okta-signin-password]",
			"okta_login_button": "[id=okta-signin-submit]"
		}
	],
	"form": [
		{
			"description": "Chrome Login Page - title",
			"dclass": ".headline h1",
			"font_family": "Roboto",
			"font_size":"24px",
			"font_weight": "400",
			"font_style": "normal",
			"font_stretch": "100%",
			"color": "#5e5e5e"
